{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "WSL Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${SO}/a.out",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        { "description": "Enable pretty-printing", "text": "-enable-pretty-printing", "ignoreFailures": true }
      ],
      "preLaunchTask": "build"
    }
  ]
}
